{
    "name": "dagpro/yii3-demo-ddd-hexagonal",
    "type": "project",
    "description": "Demo of DDD architecture and hexagonal using Yii Framework as an example",
    "keywords": [
        "yii",
        "app",
        "ddd",
        "hexagonal",
        "clean-architecture"
    ],
    "homepage": "https://github.com/dagpro/yii3-demo-ddd-hexagonal",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/demo",
        "issues": "https://github.com/yiisoft/demo/issues",
        "forum": "https://www.yiiframework.com/forum/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.4",
        "ext-filter": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "cycle/database": "^2.3",
        "cycle/entity-behavior": "^1.4",
        "cycle/orm": "^v2.10",
        "doctrine/collections": "^2.3",
        "fakerphp/faker": "^v1.24",
        "httpsoft/http-message": "^1.1",
        "psr/container": "2.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^3.0",
        "symfony/console": "^v7.2",
        "vlucas/phpdotenv": "^5.6",
        "yiisoft/access": "^2.0",
        "yiisoft/aliases": "^3.1",
        "yiisoft/assets": "^5.1",
        "yiisoft/auth": "^3.1",
        "yiisoft/bootstrap5": "^1.0",
        "yiisoft/cache": "^3.1",
        "yiisoft/cache-file": "^3.2",
        "yiisoft/config": "1.6",
        "yiisoft/cookies": "^1.2.3",
        "yiisoft/csrf": "^2.2",
        "yiisoft/data": "dev-master",
        "yiisoft/data-cycle": "dev-master",
        "yiisoft/data-response": "^2.1",
        "yiisoft/definitions": "^3.4",
        "yiisoft/di": "^1.3",
        "yiisoft/error-handler": "^4.2",
        "yiisoft/form": "^1.4",
        "yiisoft/form-model": "^1.0.1",
        "yiisoft/html": "^3.11",
        "yiisoft/http": "^1.2",
        "yiisoft/http-middleware": "^1.1",
        "yiisoft/input-http": "^1.0.1",
        "yiisoft/log": "^2.1",
        "yiisoft/log-target-file": "^3.0",
        "yiisoft/mailer": "^6.0",
        "yiisoft/middleware-dispatcher": "^5.3",
        "yiisoft/rate-limiter": "dev-master",
        "yiisoft/rbac": "^2.1",
        "yiisoft/rbac-php": "^2.0",
        "yiisoft/router": "^4.0",
        "yiisoft/router-fastroute": "^4.0",
        "yiisoft/security": "^1.1",
        "yiisoft/session": "^3.0",
        "yiisoft/strings": "2.6.0",
        "yiisoft/translator": "^3.1",
        "yiisoft/translator-message-php": "^1.0",
        "yiisoft/translator-extractor": "^2.0",
        "yiisoft/user": "^2.3",
        "yiisoft/validator": "2.5",
        "yiisoft/view": "^12.2",
        "yiisoft/widget": "^2.2",
        "yiisoft/yii-console": "^2.3",
        "yiisoft/yii-cycle": "^2.0",
        "yiisoft/yii-dataview": "dev-master",
        "yiisoft/yii-event": "^2.1",
        "yiisoft/yii-http": "^1.0",
        "yiisoft/yii-middleware": "1.1",
        "yiisoft/yii-runner-console": "^2.2",
        "yiisoft/yii-runner-http": "^3.2",
        "yiisoft/yii-swagger": "^2.2",
        "yiisoft/yii-view-renderer": "^7.3"
    },
    "require-dev": {
        "codeception/c3": "^2.9",
        "codeception/codeception": "^5.3",
        "codeception/module-asserts": "^3.0",
        "codeception/module-cli": "^2.0.0",
        "codeception/module-phpbrowser": "^3.0",
        "phpunit/phpunit": "^12.3",
        "roave/infection-static-analysis-plugin": "^1.35.0",
        "roave/security-advisories": "dev-master",
        "spatie/phpunit-watcher": "^1.24.0",
        "vimeo/psalm": "^6.10",
        "rector/rector": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.88.0",
        "shipmonk/composer-dependency-analyser": "^1.8.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests"
        }
    },
    "scripts": {
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "./yii serve"
        ],
        "post-update-cmd": [
            "App\\Installer::postUpdate",
            "App\\Installer::copyEnvFile"
        ],
        "post-create-project-cmd": [
            "App\\Installer::copyEnvFile"
        ],
        "test": "codecept run",
        "test-watch": "phpunit-watcher watch",
        "cs-fix": "php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes",
        "cs-check": "php-cs-fixer fix --config=.php-cs-fixer.php --allow-risky=yes --dry-run --diff"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin-file": "configuration.php",
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "./node_modules/{$name}": [
                "type:npm-asset"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "codeception/c3": true,
            "infection/extension-installer": true,
            "composer/package-versions-deprecated": true,
            "yiisoft/config": true,
            "composer/installers": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
